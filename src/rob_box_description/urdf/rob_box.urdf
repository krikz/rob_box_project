<?xml version="1.0"?>
<robot name="rob_box" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Основное шасси -->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0.09425" rpy="0 0 0"/>
      <geometry>
        <box size="0.4757 0.238 0.1885"/> <!-- Размеры шасси (в метрах) -->
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/> <!-- Белый цвет -->
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.09425" rpy="0 0 0"/>
      <geometry>
        <box size="0.4757 0.238 0.1885"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10"/> <!-- Масса шасси -->
      <origin xyz="0 0 0.09425"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
  </link>

  <!-- Левый рокер -->
  <link name="left_rocker">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://rob_box_description/meshes/rocker.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/> <!-- Черный цвет -->
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.28934 0.05 0.01"/> <!-- Прокси для коллизии -->
      </geometry>
    </collision>
    <inertial>
      <mass value="3"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.02"/>
    </inertial>
  </link>

  <!-- Правый рокер -->
  <link name="right_rocker">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://rob_box_description/meshes/rocker.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/> <!-- Черный цвет -->
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.28934 0.05 0.01"/> <!-- Прокси для коллизии -->
      </geometry>
    </collision>
    <inertial>
      <mass value="3"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.02"/>
    </inertial>
  </link>

  <!-- Соединение левого рокера с шасси -->
  <joint name="left_rocker_joint" type="revolute">
    <parent link="base_link"/>
    <child link="left_rocker"/>
    <origin xyz="0 0.23048 0.09425" rpy="0 0 0"/> <!-- Положение рокера -->
    <axis xyz="0 1 0"/> <!-- Ось вращения -->
    <limit effort="10" velocity="1" lower="-0.5" upper="0.5"/>
  </joint>

  <!-- Соединение правого рокера с шасси -->
  <joint name="right_rocker_joint" type="revolute">
    <parent link="base_link"/>
    <child link="right_rocker"/>
    <origin xyz="0 -0.23048 0.09425" rpy="0 0 0"/> <!-- Положение рокера -->
    <axis xyz="0 1 0"/> <!-- Ось вращения -->
    <limit effort="10" velocity="1" lower="-0.5" upper="0.5"/>
  </joint>

  <!-- Колеса -->
  <link name="left_front_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.115" length="0.05"/> <!-- Радиус и ширина колеса -->
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.115" length="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <link name="left_rear_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.115" length="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.115" length="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <link name="right_front_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.115" length="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.115" length="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <link name="right_rear_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.115" length="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.115" length="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Крепление колес к левому рокеру -->
  <joint name="left_front_wheel_joint" type="continuous">
    <parent link="left_rocker"/>
    <child link="left_front_wheel"/>
    <origin xyz="0.14467 0 -0.025" rpy="0 0 0"/> <!-- Позиция крепления -->
    <axis xyz="0 1 0"/> <!-- Ось вращения -->
  </joint>

  <joint name="left_rear_wheel_joint" type="continuous">
    <parent link="left_rocker"/>
    <child link="left_rear_wheel"/>
    <origin xyz="-0.14467 0 -0.025" rpy="0 0 0"/> <!-- Позиция крепления -->
    <axis xyz="0 1 0"/> <!-- Ось вращения -->
  </joint>

  <!-- Крепление колес к правому рокеру -->
  <joint name="right_front_wheel_joint" type="continuous">
    <parent link="right_rocker"/>
    <child link="right_front_wheel"/>
    <origin xyz="0.14467 0 -0.025" rpy="0 0 0"/> <!-- Позиция крепления -->
    <axis xyz="0 1 0"/> <!-- Ось вращения -->
  </joint>

  <joint name="right_rear_wheel_joint" type="continuous">
    <parent link="right_rocker"/>
    <child link="right_rear_wheel"/>
    <origin xyz="-0.14467 0 -0.025" rpy="0 0 0"/> <!-- Позиция крепления -->
    <axis xyz="0 1 0"/> <!-- Ось вращения -->
  </joint>

</robot>